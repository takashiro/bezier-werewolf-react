{
  "name": "@bezier/werewolf-react",
  "version": "0.1.0",
  "description": "Web Client of One Night Ultimate Werewolf (React.js)",
  "type": "module",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack",
    "debug": "webpack --mode=development",
    "test": "npm run lint && playwright test",
    "lint": "eslint --ext=ts,tsx,js --max-warnings=0 .",
    "update-messages": "linguist update --config=.linguistrc.cjs",
    "release-messages": "linguist release --config=.linguistrc.cjs",
    "prune-messages": "linguist prune --config=.linguistrc.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takashiro/bezier-werewolf-react.git"
  },
  "keywords": [
    "one",
    "night",
    "werewolf",
    "react"
  ],
  "author": "Kazuichi Takashiro",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/takashiro/bezier-werewolf-react/issues"
  },
  "homepage": "https://github.com/takashiro/bezier-werewolf-react#readme",
  "dependencies": {
    "@bezier/werewolf-client": "^0.6.0",
    "@bezier/werewolf-core": "^0.6.1",
    "@karuta/rest-client": "^0.5.0",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.2"
  },
  "devDependencies": {
    "@bezier/werewolf-server": "^0.6.0",
    "@karuta/linguist": "^0.7.2",
    "@playwright/test": "^1.41.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
